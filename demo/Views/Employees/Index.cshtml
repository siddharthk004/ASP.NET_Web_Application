@model IEnumerable<demo.Models.Employee>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Ename)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Etype)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Eaddr)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Emob)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Edesign)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Ename)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Etype)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Eaddr)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Emob)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Edesign)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Eid }) |
            @Html.ActionLink("Details", "Details", new { id=item.Eid }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Eid })
        </td>
    </tr>
}

</table>
